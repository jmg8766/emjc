(MAIN-CLASS (ID Factorial)
(MAIN-MTD-DECLR STRING[] (ID a)
(PRINT (FUN-CALL (ID ComputeFac) (INTLIT 10)))
)
(CLASS (ID Fac)
(MTD-DECL (ID ComputeFac) (TY-ID-LIST (INT ((ID num))))
(BLOCK

(VAR-DECL INT (ID num_aux))
(IF (< (ID num) (INTLIT 1))
	(EQSIGN (ID num_aux) (INTLIT 1))
	(EQSIGN (ID num_aux) (* (ID num) (FUN-CALL (ID ComputeFac) (- (ID num) (INTLIT 1)))))
))
(RETURN (ID num_aux)))
)