(MAIN-CLASS (ID QuickSort)
(MAIN-MTD-DECLR STRING[] (ID a)
(PRINT (FUN-CALL (ID Start) (INTLIT 10)))
)
(CLASS (ID QS)
(VAR-DECL INT[] (ID number))
(VAR-DECL INT (ID size))
(MTD-DECL (ID Start) (TY-ID-LIST (INT ((ID sz))))
(BLOCK

(VAR-DECL INT (ID aux01))
(EQSIGN (ID aux01) (FUN-CALL (ID Init) (ID sz)))
(EQSIGN (ID aux01) (FUN-CALL (ID Print)))
(PRINT (INTLIT 9999))
(EQSIGN (ID aux01) (- (ID size) (INTLIT 1)))
(EQSIGN (ID aux01) (FUN-CALL (ID Sort) (INTLIT 0) (ID aux01)))
(EQSIGN (ID aux01) (FUN-CALL (ID Print))))
(RETURN (INTLIT 0)))
(MTD-DECL (ID Sort) (TY-ID-LIST (INT ((ID left)))(INT ((ID right))))
(BLOCK

(VAR-DECL INT (ID v))
(VAR-DECL INT (ID i))
(VAR-DECL INT (ID j))
(VAR-DECL INT (ID nt))
(VAR-DECL INT (ID t))
(VAR-DECL BOOLEAN (ID cont01))
(VAR-DECL BOOLEAN (ID cont02))
(VAR-DECL INT (ID aux03))
(EQSIGN (ID t) (INTLIT 0))
(IF (< (ID left) (ID right))
	(BLOCK

(EQSIGN (ID v) (ARRAY-INDEX (ID number) (ID right)))
(EQSIGN (ID i) (- (ID left) (INTLIT 1)))
(EQSIGN (ID j) (ID right))
(EQSIGN (ID cont01) true)
(WHILE (ID cont01)
	(BLOCK

(EQSIGN (ID cont02) true)
(WHILE (ID cont02)
	(BLOCK

(EQSIGN (ID i) (+ (ID i) (INTLIT 1)))
(EQSIGN (ID aux03) (ARRAY-INDEX (ID number) (ID i)))
(IF (NOT (< (ID aux03) (ID v)))
	(EQSIGN (ID cont02) false)
	(EQSIGN (ID cont02) true)
))
)
(EQSIGN (ID cont02) true)
(WHILE (ID cont02)
	(BLOCK

(EQSIGN (ID j) (- (ID j) (INTLIT 1)))
(EQSIGN (ID aux03) (ARRAY-INDEX (ID number) (ID j)))
(IF (NOT (< (ID v) (ID aux03)))
	(EQSIGN (ID cont02) false)
	(EQSIGN (ID cont02) true)
))
)
(EQSIGN (ID t) (ARRAY-INDEX (ID number) (ID i)))
(EQSIGN (ARRAY-INDEX (ID number) (ID i)) (ARRAY-INDEX (ID number) (ID j)))
(EQSIGN (ARRAY-INDEX (ID number) (ID j)) (ID t))
(IF (< (ID j) (+ (ID i) (INTLIT 1)))
	(EQSIGN (ID cont01) false)
	(EQSIGN (ID cont01) true)
))
)
(EQSIGN (ARRAY-INDEX (ID number) (ID j)) (ARRAY-INDEX (ID number) (ID i)))
(EQSIGN (ARRAY-INDEX (ID number) (ID i)) (ARRAY-INDEX (ID number) (ID right)))
(EQSIGN (ARRAY-INDEX (ID number) (ID right)) (ID t))
(EQSIGN (ID nt) (FUN-CALL (ID Sort) (ID left) (- (ID i) (INTLIT 1))))
(EQSIGN (ID nt) (FUN-CALL (ID Sort) (+ (ID i) (INTLIT 1)) (ID right))))
	(EQSIGN (ID nt) (INTLIT 0))
))
(RETURN (INTLIT 0)))
(MTD-DECL (ID Print) (TY-ID-LIST )
(BLOCK

(VAR-DECL INT (ID j))
(EQSIGN (ID j) (INTLIT 0))
(WHILE (< (ID j) (ID size))
	(BLOCK

(PRINT (ARRAY-INDEX (ID number) (ID j)))
(EQSIGN (ID j) (+ (ID j) (INTLIT 1))))
))
(RETURN (INTLIT 0)))
(MTD-DECL (ID Init) (TY-ID-LIST (INT ((ID sz))))
(BLOCK

(EQSIGN (ID size) (ID sz))
(EQSIGN (ID number) (NEW-INT[](ID sz)))
(EQSIGN (ARRAY-INDEX (ID number) (INTLIT 0)) (INTLIT 20))
(EQSIGN (ARRAY-INDEX (ID number) (INTLIT 1)) (INTLIT 7))
(EQSIGN (ARRAY-INDEX (ID number) (INTLIT 2)) (INTLIT 12))
(EQSIGN (ARRAY-INDEX (ID number) (INTLIT 3)) (INTLIT 18))
(EQSIGN (ARRAY-INDEX (ID number) (INTLIT 4)) (INTLIT 2))
(EQSIGN (ARRAY-INDEX (ID number) (INTLIT 5)) (INTLIT 11))
(EQSIGN (ARRAY-INDEX (ID number) (INTLIT 6)) (INTLIT 6))
(EQSIGN (ARRAY-INDEX (ID number) (INTLIT 7)) (INTLIT 9))
(EQSIGN (ARRAY-INDEX (ID number) (INTLIT 8)) (INTLIT 19))
(EQSIGN (ARRAY-INDEX (ID number) (INTLIT 9)) (INTLIT 5)))
(RETURN (INTLIT 0)))
)