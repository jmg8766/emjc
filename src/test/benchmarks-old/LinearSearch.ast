(MAIN-CLASS (ID LinearSearch)
(MAIN-MTD-DECLR (ID a)
(PRINT (DOT (NEW-OBJECT (ID LS)) (FUN-CALL (ID Start) (INTLIT 10))))
)
(CLASS (ID LS)
(VAR-DECL INT[] (ID number))
(VAR-DECL INT (ID size))
(MTD-DECL INT (ID Start) (TY-ID-LIST (INT (ID sz)))
(BLOCK
(VAR-DECL INT (ID aux01))
(VAR-DECL INT (ID aux02))
(EQSIGN (ID aux01) (DOT THIS (FUN-CALL (ID Init) (ID sz))))
(EQSIGN (ID aux02) (DOT THIS (FUN-CALL (ID Print))))
(PRINT (INTLIT 9999))
(PRINT (DOT THIS (FUN-CALL (ID Search) (INTLIT 8))))
(PRINT (DOT THIS (FUN-CALL (ID Search) (INTLIT 12))))
(PRINT (DOT THIS (FUN-CALL (ID Search) (INTLIT 17))))
(PRINT (DOT THIS (FUN-CALL (ID Search) (INTLIT 50))))
(RETURN (INTLIT 55))))
(MTD-DECL INT (ID Print) (TY-ID-LIST )
(BLOCK
(VAR-DECL INT (ID j))
(EQSIGN (ID j) (INTLIT 1))
(WHILE (< (ID j) (ID size))
	(BLOCK
(PRINT (ARRAY-INDEX (ID number) (ID j)))
(EQSIGN (ID j) (+ (ID j) (INTLIT 1))))
)
(RETURN (INTLIT 0))))
(MTD-DECL INT (ID Search) (TY-ID-LIST (INT (ID num)))
(BLOCK
(VAR-DECL INT (ID j))
(VAR-DECL BOOLEAN (ID ls01))
(VAR-DECL INT (ID ifound))
(VAR-DECL INT (ID aux01))
(VAR-DECL INT (ID aux02))
(VAR-DECL INT (ID nt))
(EQSIGN (ID j) (INTLIT 1))
(EQSIGN (ID ls01) false)
(EQSIGN (ID ifound) (INTLIT 0))
(WHILE (< (ID j) (ID size))
	(BLOCK
(EQSIGN (ID aux01) (ARRAY-INDEX (ID number) (ID j)))
(EQSIGN (ID aux02) (+ (ID num) (INTLIT 1)))
(IF (< (ID aux01) (ID num))
	(EQSIGN (ID nt) (INTLIT 0))
	(IF (NOT (< (ID aux01) (ID aux02)))
	(EQSIGN (ID nt) (INTLIT 0))
	(BLOCK
(EQSIGN (ID ls01) true)
(EQSIGN (ID ifound) (INTLIT 1))
(EQSIGN (ID j) (ID size)))
)
)
(EQSIGN (ID j) (+ (ID j) (INTLIT 1))))
)
(RETURN (ID ifound))))
(MTD-DECL INT (ID Init) (TY-ID-LIST (INT (ID sz)))
(BLOCK
(VAR-DECL INT (ID j))
(VAR-DECL INT (ID k))
(VAR-DECL INT (ID aux01))
(VAR-DECL INT (ID aux02))
(EQSIGN (ID size) (ID sz))
(EQSIGN (ID number) (NEW-INT[](ID sz)))
(EQSIGN (ID j) (INTLIT 1))
(EQSIGN (ID k) (+ (ID size) (INTLIT 1)))
(WHILE (< (ID j) (ID size))
	(BLOCK
(EQSIGN (ID aux01) (* (INTLIT 2) (ID j)))
(EQSIGN (ID aux02) (- (ID k) (INTLIT 3)))
(EQSIGN (ARRAY-INDEX (ID number) (ID j)) (+ (ID aux01) (ID aux02)))
(EQSIGN (ID j) (+ (ID j) (INTLIT 1)))
(EQSIGN (ID k) (- (ID k) (INTLIT 1))))
)
(RETURN (INTLIT 0)))))