(MAIN-CLASS (ID Factorial)
(MAIN-MTD-DECLR (ID a) (BLOCK
(PRINT (DOT (NEW-OBJECT (ID Fac)) (FUN-CALL (INTLIT 10))))
))

(CLASS (ID Fac)
(MTD-DECL INT (ID ComputeFac) (TY-ID-LIST (INT (ID num)))(BLOCK
	(VAR-DECL INT (ID num_aux))
	(IF (< (ID num) (INTLIT 1))
		(EQSIGN (ID num_aux) (INTLIT 1))
		(EQSIGN (ID num_aux) (* (ID num) (DOT THIS (FUN-CALL (- (ID num) (INTLIT 1))))))
	)	
(RETURN (ID num_aux)))))