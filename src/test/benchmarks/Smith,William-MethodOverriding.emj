class MethodOverriding {
	public static void main (String[] args) {
		Rectangle A;
		A = new Rectangle();
		Square B;
		B= new Square();
		
		A.setHeight(5);
		A.setLength(10);
		B.setHeight(5);
		B.setLength(5);
		
		int aArea;
		aArea = A.area();
		int aPerim;
		aPerim = A.perimeter();
		int bArea;
		bArea = B.area();
		int bPerim;
		bPerim = B.perimeter();
		
		System.out.println("A Area: " + aArea);
		System.out.println("A Perimeter: " + aPerim);
		System.out.println("B Area: " + bArea);
		System.out.println("B Perimeter: " + bPerim);
	}
}

class Rectangle {
	int height;
	int length;
	
	public int setHeight(int h) {
		this.height = h;
		return 0;
	}
	
	public int setLength(int l) {
		this.length = l;
		return 0;
	}
	
	public int area() {
		return this.length * this.height;
	}
	
	public int perimeter() {
		return this.length * 2 + this.height * 2;
	}
}

class Square extends Rectangle {
	public int area() {
		System.out.println("I'm a square!");
		return this.length * this.height;
	}
	
	public int perimeter() {
		System.out.println("My sides are all equal");
		return this.length * 4;
	}
}